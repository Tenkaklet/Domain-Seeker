{
  "name": "domain-seeker",
  "version": "0.0.2",
  "description": "Utilises the user's ability to search for available domains",
  "keywords": [
    "oclif",
    "CLI",
    "domain",
    "checker"
  ],
  "homepage": "https://github.com/Tenkaklet/Domain-Seeker",
  "bugs": "https://github.com/Tenkaklet/Domain-Seeker/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Tenkaklet/Domain-Seeker.git"
  },
  "license": "MIT",
  "author": "Max Carlquist",
  "type": "module",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "domain-seeker": "bin/run.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json",
    "/theme.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "npm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "version": "oclif readme && git add README.md"
  },
  "oclif": {
    "theme": "theme.json",
    "bin": "domain-seeker",
    "commands": "./dist/commands",
    "dirname": "domain-seeker",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "domain": {
        "description": "Check if a domain is available"
      }
    }
  },
  "dependencies": {
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "ansi-colors": "^4.1.3",
    "chalk": "^5.3.0",
    "cli-progress": "^3.12.0",
    "color-json": "^3.0.5",
    "dotenv": "^16.4.5",
    "inquirer": "^9.2.19",
    "node-fetch": "^3.3.2",
    "ora": "^8.0.1",
    "terminal-link": "^3.0.0",
    "tty-table": "^4.2.3"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@types/chai": "^4",
    "@types/inquirer": "^9.0.7",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.13.0",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
